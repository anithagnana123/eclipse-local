package simplilearn.examples;

public class Casting {

	public static void main(String[] args) {
		
	//int to long	
      int i1 =56;
      long l1 = i1;
      System.out.println("The result is "+l1);
		
    //long to int
      long l2 =56L;
      int i2 = (int)l2;
      System.out.println("The result is "+i2);
    
    //double to int
    
    
    //int to byte
    
    
    //long to float
    
    
    
	}

}
 